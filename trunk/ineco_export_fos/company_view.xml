<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

       <record model="ir.ui.view" id="view_ineco_fos_company_form">
            <field name="name">ineco.fos.res.company.form</field>
            <field name="model">res.company</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">
                <xpath expr="/form/notebook" position="inside">
                    <page string="OMG SQL Server">
	                	<field name="fos_dbname" />
	                	<field name="fos_host" />
	                	<field name="fos_user" />
	                	<field name="fos_password" />
                    </page>
                </xpath>
            </field>
        </record>

	</data>
</openerp>