<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

       <record model="ir.ui.view" id="view_ineco_department_export_form">
            <field name="name">ineco.hr.department.export.form</field>
            <field name="model">hr.department</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="hr.view_department_form" />
            <field name="arch" type="xml">
                <field name="name" position="after">
                	<field name="code_nav" />
                	<field name="nav_id" />
                </field>                
            </field>
        </record>

       <record model="ir.ui.view" id="view_ineco_department_export_tree">
            <field name="name">ineco.hr.department.export.tree</field>
            <field name="model">hr.department</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="hr.view_department_tree" />
            <field name="arch" type="xml">
                <field name="complete_name" position="after">
                	<field name="code_nav" />
                	<field name="nav_id" />
                </field>                
            </field>
        </record>

	</data>
</openerp>