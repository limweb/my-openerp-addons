<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="seq_stock_journal_type_raw" model="ir.sequence.type">
            <field name="name">RAW Material</field>
            <field name="code">ineco.type.stock.journal.raw</field>
        </record>

        <record id="seq_stock_journal_type_wip" model="ir.sequence.type">
            <field name="name">WIP</field>
            <field name="code">ineco.type.stock.journal.wip</field>
        </record>
        
        <record id="seq_stock_journal_type_hr" model="ir.sequence.type">
            <field name="name">HR</field>
            <field name="code">ineco.type.stock.journal.hr</field>
        </record>
        
        <record id="seq_stock_journal_type_fa" model="ir.sequence.type">
            <field name="name">FG</field>
            <field name="code">ineco.type.stock.journal.fa</field>
        </record>

        <record id="seq_stock_journal_type_wip_fd" model="ir.sequence.type">
            <field name="name">WIP FD</field>
            <field name="code">ineco.type.stock.wip.fd</field>
        </record>

        <record id="seq_stock_journal_type_wip_ds" model="ir.sequence.type">
            <field name="name">WIP DS</field>
            <field name="code">ineco.type.stock.wip.ds</field>
        </record>

        <record id="seq_stock_journal_type_fg_fd" model="ir.sequence.type">
            <field name="name">FG FD</field>
            <field name="code">ineco.type.stock.fg.fd</field>
        </record>

        <record id="seq_stock_journal_type_fg_ds" model="ir.sequence.type">
            <field name="name">FG DS</field>
            <field name="code">ineco.type.stock.fg.ds</field>
        </record>


        
        <!-- Sequence -->
        <record id="seq_stock_journal_material" model="ir.sequence">
            <field name="name">Raw Material Sequence</field>
            <field name="code">ineco.type.stock.journal.raw</field>
            <field eval="5" name="padding"/>
            <field name="prefix">RIN%(y)s%(month)s-</field>
        </record>
        
        <record id="seq_stock_journal_wip" model="ir.sequence">
            <field name="name">WIP Sequence</field>
            <field name="code">ineco.type.stock.journal.wip</field>
            <field eval="5" name="padding"/>
            <field name="prefix">WIN%(y)s%(month)s-</field>
        </record>

        <record id="seq_stock_journal_fa" model="ir.sequence">
            <field name="name">FG Sequence</field>
            <field name="code">ineco.type.stock.journal.fa</field>
            <field eval="5" name="padding"/>
            <field name="prefix">FIN%(y)s%(month)s-</field>
        </record>
        
        <record id="seq_stock_journal_hr" model="ir.sequence">
            <field name="name">HR Sequence</field>
            <field name="code">ineco.type.stock.journal.hr</field>
            <field eval="5" name="padding"/>
            <field name="prefix">HIN%(y)s%(month)s-</field>
        </record>

        <record id="seq_stock_journal_wip_fd" model="ir.sequence">
            <field name="name">Transfer WIP FD Sequence</field>
            <field name="code">ineco.type.stock.wip.fd</field>
            <field eval="5" name="padding"/>
            <field name="prefix">WIP-FDIN%(y)s%(month)s-</field>
        </record>

        <record id="seq_stock_journal_wip_ds" model="ir.sequence">
            <field name="name">Transfer WIP DS Sequence</field>
            <field name="code">ineco.type.stock.wip.ds</field>
            <field eval="5" name="padding"/>
            <field name="prefix">WIP-DSIN%(y)s%(month)s-</field>
        </record>

        <record id="seq_stock_journal_fg_fd" model="ir.sequence">
            <field name="name">Transfer FG FD Sequence</field>
            <field name="code">ineco.type.stock.fg.fd</field>
            <field eval="5" name="padding"/>
            <field name="prefix">FG-FDIN%(y)s%(month)s-</field>
        </record>

        <record id="seq_stock_journal_fg_ds" model="ir.sequence">
            <field name="name">Transfer FG DS Sequence</field>
            <field name="code">ineco.type.stock.fg.ds</field>
            <field eval="5" name="padding"/>
            <field name="prefix">FG-DSIN%(y)s%(month)s-</field>
        </record>

    </data>
</openerp>
