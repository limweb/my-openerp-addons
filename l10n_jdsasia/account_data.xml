    <field name="type">percent</field>
    <field name="amount" eval="0.07"/>
    <field name="account_collected_id" ref="113001"/>
    <field name="account_paid_id" ref="113001"/>
    <field name="base_code_id" ref="jdsasia_tc_base_input"/>
    <field name="tax_code_id" ref="jdsasia_tc_tax_vat_input"/>
    <field name="type_tax_use">purchase</field>
