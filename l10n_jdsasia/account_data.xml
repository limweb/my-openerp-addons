<!-- CHART OF TAXES -->

<record id="jdsasia_tc_root" model="account.tax.code.template">
    <field name="name">JDS ASIA - Chart of Taxes</field>
    <field name="parent_id" eval="False"/>
</record>

<!-- TAXABLE AMOUNTS -->

<record id="jdsasia_tc_base" model="account.tax.code.template">
    <field name="name">Taxable amounts</field>
    <field name="parent_id" ref="jdsasia_tc_root"/>
</record>

<!-- VAT -->

<record id="jdsasia_tc_base_vat" model="account.tax.code.template">
    <field name="name">Value Added Tax</field>
    <field name="parent_id" eval="jdsasia_tc_base"/>
</record>

<record id="jdsasia_tc_base_output" model="account.tax.code.template">
    <field name="name">Output VAT</field>
    <field name="code">outvat</field>
    <field name="parent_id" ref="jdsasia_tc_base_vat"/>
</record>

<record id="jdsasia_tc_base_input" model="account.tax.code.template">
    <field name="name">Input VAT</field>
    <field name="code">invat</field>
    <field name="parent_id" ref="jdsasia_tc_base_vat"/>
</record>

<!-- TAXES TO BE PAID -->

<!-- VAT -->

<record id="jdsasia_tc_tax" model="account.tax.code.template">
    <field name="name">Taxes to be paid</field>
    <field name="parent_id" ref="jdsasia_tc_root"/>
</record>

<record id="jdsasia_tc_tax_vat" model="account.tax.code.template">
    <field name="name">Value Added Tax</field>
    <field name="parent_id" eval="jdsasia_tc_tax"/>
</record>

<record id="jdsasia_tc_tax_vat_output" model="account.tax.code.template">
    <field name="name">Output VAT</field>
    <field name="code">PP30_5</field>
    <field name="parent_id" ref="jdsasia_tc_tax_vat"/>
</record>

<record id="jdsasia_tc_tax_vat_input" model="account.tax.code.template">
    <field name="name">Input VAT</field>
    <field name="code">PP30_7</field>
    <field name="parent_id" ref="jdsasia_tc_tax_vat"/>
    <field name="sign" eval="-1"/>
</record>

<!-- CHART OF ACCOUNTS/TAXES -->

<record id="jdsasia_chart" model="account.chart.template">
    <field name="name">JDS ASIA - Chart of Accounts</field>
    <field name="account_root_id" ref="jdsasia_root"/>
    <field name="tax_code_root_id" ref="jdsasia_tc_root"/>

    <field name="bank_account_view_id" ref="111100"/>

    <field name="property_account_receivable" ref="112100"/>
    <field name="property_account_payable" ref="211001"/>

    <field name="property_account_expense_categ" ref="510300"/>
    <field name="property_account_income_categ" ref="410000"/>

    <field name="property_account_expense" ref="510300"/>
    <field name="property_account_income" ref="410000"/>
</record>

